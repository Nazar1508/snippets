{
	"svg sprite": {
		"prefix": "svg sprite",
		"body": [
			"<svg class=\"$1\" width=\"$2\" height=\"$3\">",
			"  <use xlink:href=\"#$4\"></use>",
			"</svg>"
		]
	},

	"picture": {
		"prefix": "picture",
		"body": [
			"<picture>",
			"  <source media=\"(max-width: $5px)\" srcset=\"img/$6 1x, img/$7 2x\">",
			"  <img class=\"$1\" src=\"img/$2\" srcset=\"img/$3 2x\" alt=\"$4\">",
			"</picture>"
		]
	},

	"picture webp": {
		"prefix": "webp",
		"body": [
			"<source type=\"image/webp\" media=\"(min-width: $1px)\" srcset=\"img/webp/$2 1x, img/webp/$3 2x\">"
		]
	},

	"source": {
		"prefix": "source",
		"body": [
			"  <source media=\"(max-width: $5px)\" srcset=\"img/$6 1x, img/$7 2x\">"
		]
	},

	"favicon": {
		"prefix": "favicon",
		"body": [
			"<link rel=\"shortcut icon\" href=\"img/icon/favicon.ico\">"
		]
	}
}
